  <rd:ReportID>b5e9566b-5c98-48c1-81b8-e0a2dd311153</rd:ReportID>
  <DataSets>
    <DataSet Name="StoreRecordCount">
      <Fields>
        <Field Name="LocationNo">
          <DataField>LocationNo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CountIt">
          <DataField>CountIt</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Development</DataSourceName>
        <CommandText>BEGIN
CREATE TABLE #LOCS(LocationNo CHAR(10))
	BEGIN
		INSERT #LOCS
		SELECT DISTINCT LocationNo
		FROM ReportsData..SipsProductInventory
	END

SELECT h.LocationNo, 
       COUNT(h.LocationNo) CountIt
FROM ReportsData..SipsProductInventory h inner join
     #Locs l ON h.LocationNo = l.LocationNo     

GROUP BY h.LocationNo
HAVING Count(h.LocationNo) &gt; 0
Order by CountIt desc

DROP TABLE #LOCS
END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
