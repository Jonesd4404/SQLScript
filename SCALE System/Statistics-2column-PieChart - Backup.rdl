      </Fields>
      <Query>
        <DataSourceName>ILS</DataSourceName>
        <CommandText>="select " &amp;
	"dbo.RSCMfn_RtrvResource(isnull(field.resource_key, field.name), 'Text', '" &amp; User!Language &amp; "') name," &amp;
	"isnull(val.value, 0) value," &amp;
	"isnull(field.column_position, 0) column_position," &amp;
	"isnull(field.format_string, 'g') format_string " &amp;
"from " &amp;
	"statistics_source source " &amp;

	"inner join statistics_group grp " &amp;
	"on " &amp;
		"grp.source_id = source.object_id " &amp;
		"and " &amp;
		"grp.sequence = " &amp; Parameters!GroupSequence.Value &amp; " " &amp;
	
	"inner join statistics_field field " &amp;
	"on " &amp;
		"field.source_id = source.object_id " &amp;
		"and " &amp;
		"field.group_id = grp.object_id " &amp;
		"and " &amp;
		"field.chart_id is null " &amp;

	"left outer join statistics_value val " &amp;
	"on " &amp;
		"val.field_id = field.object_id " &amp;
		"and " &amp;
		"val.source_key = '" &amp; Parameters!SourceKey.Value &amp; "' " &amp;
"where " &amp;
	"source.name = '" &amp; Parameters!SourceName.Value &amp; "' " &amp;
"order by " &amp;
	"field.row_position, field.column_position"</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
