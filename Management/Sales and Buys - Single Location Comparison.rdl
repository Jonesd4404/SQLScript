  <Code>Public Function GetPctDiff(ByRef CurrentVal As Double,ByRef PreviousVal As Double)As String
	dim PctDiff as Double
	dim PctFormat As String
	IF PreviousVal &lt;&gt; 0 THEN
		PctDiff = ((CurrentVal - PreviousVal)/PreviousVal)
		PctFormat = FormatPercent(PctDiff, 1, True, False, True)
	ELSEIF CurrentVal &lt;&gt; 0 AND PreviousVal = 0 THEN
		PctFormat = " 100.0"
	ELSEIF CurrentVal = 0 AND PreviousVal = 0 THEN
		PctFormat = " 0.0"
	END IF
	Return  PctFormat
    End Function</Code>
