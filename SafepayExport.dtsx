                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @UPLOADDATE DATETIME

SET @UPLOADDATE = (SELECT GETDATE ()  )

SELECT 

TRANSTYPE = 
	CASE A.CMTrxType
	WHEN 4 THEN 'V'
	ELSE 'C'
	END,

D.BNKACTNM,
A.TRXDATE,
A.CMTrxNum,
A.TRXAMNT,
A.paidtorcvdfrom,
C.ADDRESS1
-- FROM THIS POINT DOWN ONLY FOR CONFIRMATION PURPOSES
/*
B.CMLinkID,
C.VNDCHKNM,
A.MEUPLDID,
A.CHEKBKID,
A.CMTrxType
*/


FROM ME123504 A INNER JOIN CM20200 B ON A.CHEKBKID = B.CHEKBKID AND A.CMTrxNum = B.CMTrxNum INNER JOIN PM00200 C ON B.CMLinkID = C.VENDORID INNER JOIN CM00100 D ON A.CHEKBKID = D.CHEKBKID

WHERE YEAR(A.MEUPDT) = YEAR(@UPLOADDATE) AND MONTH(A.MEUPDT) = MONTH(@UPLOADDATE) AND DAY(A.MEUPDT) = DAY(@UPLOADDATE)</property>
